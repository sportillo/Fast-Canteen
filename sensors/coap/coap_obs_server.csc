      se.sics.cooja.mspmote.Z1MoteType
      <identifier>z13</identifier>
      <description>Z1 Mote Type #z13</description>
      <source EXPORT="discard">[CONTIKI_DIR]/examples/LAB/coap/coap_obs_server.c</source>
      <commands EXPORT="discard">make coap_obs_server.z1 TARGET=z1</commands>
      <firmware EXPORT="copy">[CONTIKI_DIR]/examples/LAB/coap/coap_obs_server.z1</firmware>
