      <moterelations>true</moterelations>
      <skin>se.sics.cooja.plugins.skins.IDVisualizerSkin</skin>
      <skin>se.sics.cooja.plugins.skins.UDGMVisualizerSkin</skin>
      <viewport>1.988939327339348 0.0 0.0 1.988939327339348 64.02639911436634 218.29572855048505</viewport>
    </plugin_config>
    <width>400</width>
    <z>1</z>
    <height>400</height>
